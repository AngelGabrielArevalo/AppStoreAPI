{
  "name": "storeappapi",
  "version": "1.0.0",
  "description": "API para tienda online.",
  "main": "app.js",
  "scripts": {
    "start:dev": "ts-node-dev src/app.ts",
    "test": "jest --runInBand",
    "test-coverage": "jest --coverage",
    "lint": "eslint . --ext .ts --fix",
    "prettier": "npx prettier --write **/*.js **/*.ts **/*.json **/*.yaml",
    "prettier-check": "prettier --check **/*.js **/*.ts **/*.json **/*.yaml",
    "typeorm": "typeorm-ts-node-esm -d ./src/configuration/database/migrationsconfig.ts",
    "m:gen": "npm run typeorm migration:generate -- src/configuration/database/migrations/",
    "m:run": "npm run typeorm migration:run"
  },
  "author": "Angel Arevalo",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "joi-class-decorators": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.7.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
